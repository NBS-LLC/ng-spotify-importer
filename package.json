{
  "name": "ng-slacker-to-spotify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve & echo $! > .ng-serve.pid",
    "stop": "kill $(cat .ng-serve.pid)",
    "version:update": "npm --no-git-tag-version version $GITHUB_REF_NAME",
    "version:inject": "cross-var replace \"@APP_VERSION@\" $npm_package_version src/app/app-info.ts",
    "build": "ng build --configuration=$ENVIRONMENT --base-href /projects/ng-spotify-importer/",
    "test": "ng test",
    "e2e": "ng e2e",
    "e2e:unit": "TS_NODE_PROJECT=e2e/tsconfig.json mocha -r ts-node/register 'e2e/**/*.spec.ts'",
    "e2e:debug": "DEBUG=true npm run e2e",
    "upload": "TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/upload.ts",
    "deploy": "npm run build && npm run upload",
    "deploy:test": "wdio run e2e/wdio.conf.ts --autoCompileOpts.tsNodeOpts.project=e2e/tsconfig.json --spec e2e/test/deploy-specs/**/*.ts"
  },
  "browser": {
    "crypto": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.1",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "crypto-js": "^4.1.1",
    "fast-xml-parser": "^4.0.3",
    "he": "^1.2.0",
    "papaparse": "^5.2.0",
    "rxjs": "~7.5.5",
    "spotify-web-api-js": "^1.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/he": "^1.1.1",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.23",
    "@types/papaparse": "^5.0.4",
    "@types/spotify-web-api-node": "^5.0.7",
    "@types/ssh2-sftp-client": "^7.0.1",
    "@wdio/cli": "^7.19.1",
    "@wdio/local-runner": "^7.19.1",
    "@wdio/mocha-framework": "^7.19.1",
    "@wdio/schematics": "^1.2.1",
    "@wdio/spec-reporter": "^7.19.1",
    "@xmldom/xmldom": "^0.8.0",
    "chromedriver": "^90.0.0",
    "codelyzer": "^6.0.0",
    "cross-var": "^1.1.0",
    "expect": "^27.5.1",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "^9.2.1",
    "protractor": "~7.0.0",
    "replace": "^1.2.1",
    "spotify-web-api-node": "^5.0.2",
    "ssh2-sftp-client": "^7.2.3",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.3",
    "wdio-chromedriver-service": "^7.3.2"
  }
}
