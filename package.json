{
  "name": "ng-slacker-to-spotify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve & echo $! > .ng-serve.pid",
    "stop": "kill $(cat .ng-serve.pid)",
    "build": "ng build --configuration=$ENVIRONMENT --base-href /projects/ng-spotify-importer/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "wdio run e2e/wdio.conf.ts --autoCompileOpts.tsNodeOpts.project=e2e/tsconfig.json",
    "e2e:unit": "TS_NODE_PROJECT=e2e/tsconfig.json mocha -r ts-node/register e2e/**/*.spec.ts",
    "e2e:debug": "DEBUG=true npm run e2e",
    "upload": "TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/upload.ts",
    "deploy": "npm run build && npm run upload",
    "deploy:test": "npm run e2e -- --spec e2e/test/deploy-specs/**/*.ts"
  },
  "browser": {
    "crypto": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "crypto-js": "^4.1.1",
    "fast-xml-parser": "^4.0.3",
    "he": "^1.2.0",
    "papaparse": "^5.2.0",
    "rxjs": "~6.5.4",
    "spotify-web-api-js": "^1.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@types/he": "^1.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^12.11.1",
    "@types/papaparse": "^5.0.4",
    "@types/spotify-web-api-node": "^5.0.7",
    "@types/ssh2-sftp-client": "^7.0.1",
    "@wdio/cli": "^7.16.16",
    "@wdio/local-runner": "^7.16.16",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/spec-reporter": "^7.16.14",
    "@xmldom/xmldom": "^0.8.0",
    "chromedriver": "^90.0.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "^9.2.1",
    "protractor": "~7.0.0",
    "spotify-web-api-node": "^5.0.2",
    "ssh2-sftp-client": "^7.2.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "wdio-chromedriver-service": "^7.2.8"
  }
}
